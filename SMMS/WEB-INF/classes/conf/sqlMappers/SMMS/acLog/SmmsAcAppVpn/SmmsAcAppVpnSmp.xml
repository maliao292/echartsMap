	select <include refid="detailColumns"/>  from SMMS_AC_APP_VPN  where RID = #{RID} 
</select> 

<update id="deleteUpdate" parameterType="hashmap" > 
	update SMMS_AC_APP_VPN  set RECORDSTATE=#{RECORDSTATE} where RID=#{RID}
</update> 
 
<select id="updateDetail" parameterType="String" resultType="hashmap"> 
      select <include refid="detailColumns"/> from SMMS_AC_APP_VPN  where RID = #{RID} 
