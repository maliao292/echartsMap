<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="SMMS.BaseData.IdcInfo.IdcInfoBiz" namespace="/SMMS/IdcInfoBiz" extends="SMMS-default">        
		<!--进入操作界面List View Add Update -->
        <action  name="Do*" class="com.zstar.SMMS.BaseData.IdcInfo.action.{1}IdcInfoAction">
            <result>/WEB-INF/jsp/fmp/frame/common/Common{1}.jsp</result>
        </action>    
		<!--新增的保存操作-->
        <action  name="*Save" class="com.zstar.SMMS.BaseData.IdcInfo.action.SaveIdcInfoAction">
        	<result type="chain" name="inserSuccess">DoUpdate</result>
        	<result type="chain" name="updateSuccess">DoUpdate</result>
        </action>   
		<!--保存可修改的列表信息-->
        <action  name="SaveDataList" class="com.zstar.SMMS.BaseData.IdcInfo.action.SaveIdcInfoDataListAction">
        	<result type="chain" name="inserSuccess">DoUpdate</result>
        	<result type="chain" name="updateSuccess">DoUpdate</result>
        </action>  
		<!--保存联合主从表信息-->
        <action  name="SaveUniteSubTableList" class="com.zstar.SMMS.BaseData.IdcInfo.action.SaveIdcInfoUniteSubTableAction">
        	<result type="chain" name="inserSuccess">DoUpdate</result>
        	<result type="chain" name="updateSuccess">DoUpdate</result>        
        </action>
		<!--删除操作-->
        <action  name="Delete" class="com.zstar.SMMS.BaseData.IdcInfo.action.DeleteIdcInfoAction">
        </action>   
         <!--刷新服务器状态-->     
         <action  name="refreshWebserviceState" class="com.zstar.SMMS.BaseData.IdcInfo.action.RefreshWebserviceStateAction">
        </action>     
        <!--根据idc运营商信息插入数据-->
        <action  name="InsertSUser" class="com.zstar.SMMS.BaseData.IdcInfo.action.InsertSUserAction" > 
		</action>             
    </package>
</struts>
