<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="SMMS.BaseData.SmmsPendingEvent_vpn.PendingEvent_vpnBiz" namespace="/SMMS/PendingEvent_vpnBiz" extends="SMMS-default">        
		<!--进入操作界面List View Add Update -->
        <action  name="Do*" class="com.zstar.SMMS.BaseData.SmmsPendingEvent_vpn.action.{1}PendingEvent_vpnAction">
            <result>/WEB-INF/jsp/fmp/frame/common/Common{1}.jsp</result>
        </action>    
		<!--新增的保存操作-->
        <action  name="*Save" class="com.zstar.SMMS.BaseData.SmmsPendingEvent_vpn.action.SavePendingEvent_vpnAction">
        	<result type="chain" name="inserSuccess">DoUpdate</result>
        	<result type="chain" name="updateSuccess">DoUpdate</result>
        </action>   
		<!--保存可修改的列表信息-->
        <action  name="SaveDataList" class="com.zstar.SMMS.BaseData.SmmsPendingEvent_vpn.action.SavePendingEvent_vpnDataListAction">
        	<result type="chain" name="inserSuccess">DoUpdate</result>
        	<result type="chain" name="updateSuccess">DoUpdate</result>
        </action>  
		<!--保存联合主从表信息-->
        <action  name="SaveUniteSubTableList" class="com.zstar.SMMS.BaseData.SmmsPendingEvent_vpn.action.SavePendingEvent_vpnUniteSubTableAction">
        	<result type="chain" name="inserSuccess">DoUpdate</result>
        	<result type="chain" name="updateSuccess">DoUpdate</result>        
        </action>
		<!--删除操作-->
        <action  name="Delete" class="com.zstar.SMMS.BaseData.SmmsPendingEvent_vpn.action.DeletePendingEvent_vpnAction">
        </action> 
        <!-- 日志举证-->     
         <action  name="PendingEventVpn" class="com.zstar.SMMS.BaseData.SmmsPendingEvent_vpn.action.RzjzPendingEvent_vpnAction">
        </action>                   
    </package>
</struts>
